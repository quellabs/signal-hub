{
  "config": {
    "platform": {
      "php": "8.3"
    }
  },
  "require": {
    "ext-json": "*",
    "ext-mysqli": "*",
    "ext-pdo": "*",
    "ext-curl": "*",
    "ext-gd": "*",
    "ext-fileinfo": "*",
    "quellabs/contracts": "dev-main",
    "quellabs/annotation-reader": "dev-main",
    "quellabs/discover": "dev-main",
    "quellabs/dependency-injection": "dev-main",
    "quellabs/objectquel": "dev-main",
    "quellabs/sculpt": "dev-main",
    "quellabs/signal-hub": "dev-main",
    "smarty/smarty": "^4.3",
    "symfony/http-foundation": "*",
    "vlucas/phpdotenv": "*"
  },
  "autoload": {
    "psr-4": {
      "Quellabs\\Canvas\\": "src/"
    }
  },
  "extra": {
    "discover": {
      "di": {
        "providers": [
          "Quellabs\\Canvas\\Discover\\TemplateEngineServiceProvider",
          "Quellabs\\Canvas\\Discover\\ObjectQuelServiceProvider"
        ]
      },
      "template_engine": {
        "provider": "Quellabs\\Canvas\\Templating\\SmartyTemplateProvider"
      }
    }
  }
}